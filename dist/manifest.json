{
  "name": "210326_Prototype_1",
  "version": "1.0.0",
  "description": "Learning to code chrome extensions!",
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://*/",
    "http://*/"
  ],
  "content_scripts": [
    {
      "js": ["js/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
